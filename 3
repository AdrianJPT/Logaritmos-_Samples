#include <iostream>

using namespace std;

int main()
{
    int neumaticos
    cin >> neumaticos
    cout<<"Cantidad de neumaticos"

 
    if  (neumaticos > 3) 
        cout<<"Monto a pagar es: " << 145 * neumaticos;
    else if (neumaticos >= 3 and neumaticos <= 5)
        cout<<"Monto a pagar es: " << neumaticos * 138;
    else if (neumaticos <= 5)
        cout<<"Monto a pagar es: " << neumaticos * 135;
    else 
        cout << "Error"
        
    cin.get()
    cin.ignore()
    return 0;
}
